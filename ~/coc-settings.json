{
  "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown", "javascript", "graphql", "html", "yaml",  "json", "typescript.tsx", "typescriptreact", "javascriptreact", "typescript"],
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit"
  }

}
